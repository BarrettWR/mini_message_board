{
  "name": "mini-message-board",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build:css": "npx tailwindcss build -o dist/output.css",
    "watch:css": "npx tailwindcss -i ./public/stylesheets/style.css -o ./dist/output.css --watch",
    "build": "npm run build:css && npm run start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^6.2.0",
    "mongoose": "^8.0.0",
    "morgan": "~1.9.1",
    "popups": "^1.1.3",
    "pug": "2.0.0-beta11",
    "sweetalert2": "^11.9.0",
    "tailwind": "^4.0.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.3.5"
  }
}
